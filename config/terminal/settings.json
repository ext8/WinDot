{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "unbound",
            "keys": "alt+enter"
        },
        {
            "command": "unbound",
            "keys": "f11"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "ctrl+w"
        }
    ],
    "alwaysOnTop": false,
    "alwaysShowTabs": true,
    "copyFormatting": "all",
    "copyOnSelect": false,
    "defaultProfile": "{00574d9b-d6ad-42e2-b26f-2c267666bfd0}",
    "disableAnimations": true,
    "language": "en-US",
    "profiles": 
    {
        "defaults": 
        {
            "antialiasingMode": "cleartype",
            "bellStyle": "none",
            "font": 
            {
                "face": "JetBrainsMono Nerd Font",
                "size": 13,
                "weight": "medium"
            },
            "icon": "C:/Users/%username%/.config/wt/1024.png",
            "startingDirectory": "%USERPROFILE%",
            "suppressApplicationTitle": false,
            "tabTitle": "terminal"
        },
        "list": 
        [
            {
                "colorScheme": "gruvbox",
                "commandline": "C:/Users/%username%/AppData/scoop/shims/pwsh.EXE -nologo",
                "font": 
                {
                    "face": "JetBrainsMono Nerd Font Mono"
                },
                "guid": "{00574d9b-d6ad-42e2-b26f-2c267666bfd0}",
                "hidden": false,
                "icon": "C:/Users/%username%/.config/wt/1024.png",
                "name": "terminal",
                "opacity": 81,
                "startingDirectory": null,
                "tabTitle": "terminal",
                "useAcrylic": true
            }
        ]
    },
